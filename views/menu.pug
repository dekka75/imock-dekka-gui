// Bar
header.mdl-layout__header
    .mdl-layout__header-row
        span.mdl-layout-title #{title}
        .mdl-layout-spacer
        nav.mdl-navigation
            a.mdl-navigation__link(href='/') Dashboad
            a.mdl-navigation__link(href='/viewer') Viewer
            a.mdl-navigation__link(href='/users') Log-In             
            a.mdl-navigation__link(href='/about') About
        .mdl-textfield.mdl-js-textfield.mdl-textfield--expandable.mdl-textfield--floating-label.mdl-textfield--align-right
            label.mdl-button.mdl-js-button.mdl-button--icon(for='fixed-header-drawer-exp')
                i.material-icons search
            .mdl-textfield__expandable-holder
                input#fixed-header-drawer-exp.mdl-textfield__input(type='text', name='sample')             

    // Tabs
    if tabs == null || tabs == undefined
        .mdl-layout__tab-bar.mdl-js-ripple-effect
            each group, i in groupsList
                a.mdl-layout__tab(href='#scroll-tab-' + group, class= (group==active) ? 'is-active' : '') #{group}
        button#dashboard-tab-add.mdl-button.mdl-js-button.mdl-button--fab.mdl-js-ripple-effect.mdl-button--colored.mdl-shadow--4dp.mdl-color--accent
            i.material-icons(role='presentation') add
            span.visuallyhidden Add
            span.mdl-button__ripple-container
                span.mdl-ripple.is-animating(style='width: 160.392px; height: 160.392px; transform: translate(-50%, -50%) translate(23px, 37px);')

// Mobil navigation
.mdl-layout__drawer
    span.mdl-layout-title #{name}
    nav.mdl-navigation
        a.mdl-navigation__link(href='/') Dashboad
        a.mdl-navigation__link(href='/viewer') Viewer
        a.mdl-navigation__link(href='/about') About